{
  "private": true,
  "name": "tc-project",
  "version": "0.0.0",
  "scripts": {
    "start": "gulp run-dev",
    "postinstall": "install-app-deps --appDir src",
    "postpostinstall": "gulp postinstall",
    "pack": "build --appDir src",
    "dist": "build --appDir src"
  },
  "build": {
    "app-bundle-id": "zyx.gettc.tc",
    "app-category-type": "public.app-category.social-networking",
    "name": "Tc",
    "asar": true,
    "app-copyright": "Copyright 2015 Andrea Stella. All rights reserved",
    "version-string": {
      "CompanyName": "Mccxiv Software",
      "FileDescription": "Tc, the chat client for Twitch",
      "ProductName": "Tc"
    },
    "win": {
      "loadingGif": "src/assets-embed/install.gif",
      "exe": "Tc.exe",
      "setupExe": "tc-setup.exe",
      "authors": "Mccxiv Software",
      "description": "Tc, the chat client for Twitch",
      "title": "Tc, the chat client for Twitch",
      "setupIcon": "src/assets-embed/icon.ico",
      "noMsi": true
    }
  },
  "directories": {
    "buildResources": "src/assets-embed"
  },
  "dependencies": {
    "bower": "1.6.5",
    "del": "1.2.0",
    "electron-builder": "3.3.0",
    "electron-prebuilt": "0.37.4",
    "electron-rebuild": "1.0.2",
    "electron-windows-installer": "1.2.5",
    "gulp": "3.9.0",
    "gulp-add-src": "0.2.0",
    "gulp-angular-templatecache": "1.6.0",
    "gulp-base64": "0.1.2",
    "gulp-concat": "2.5.2",
    "gulp-css-base64": "1.3.2",
    "gulp-if": "1.2.5",
    "gulp-minify-css": "1.1.1",
    "gulp-ng-annotate": "0.5.3",
    "gulp-strip-debug": "1.0.2",
    "gulp-uglify": "1.2.0",
    "gulp-useref": "1.1.2",
    "root-require": "0.3.1",
    "run-sequence": "1.1.0",
    "shelljs": "^0.6.0"
  }
}
